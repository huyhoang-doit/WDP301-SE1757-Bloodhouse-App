{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"],
        "@components/*": ["src/components/*"],
        "@screens/*": ["src/screens/*"],
        "@navigators/*": ["src/navigators/*"],
        "@utils/*": ["src/utils/*"],
        "@assets/*": ["src/assets/*"],
        "@apis/*": ["src/apis/*"],
        "@constants/*": ["src/constants/*"],
        "@hooks/*": ["src/hooks/*"],
        "@store/*": ["src/store/*"],
        "@styles/*": ["src/styles/*"]
      }
    },
    "extends": "expo/tsconfig.base"
  }